SELECT CO_SABE_LER_ESCREVER, IN_FREQUENTA_ESCOLA_MEMB, CO_CURSO_FREQUENTA_MEMB, CO_ANO_SERIE_FREQUENTA_MEMB, CO_CURSO_FREQ_PESSOA_MEMB, CO_ANO_SERIE_FREQUENTOU_MEMB, CO_CONCLUIU_FREQUENTOU_MEMB 
FROM P_CADASTRO_ODS_202206.TB_ESCOLARIDADE_07 te LEFT JOIN P_CADASTRO_ODS_202206.TB_PESSOA_04 tp 
ON TP.CO_CHV_NATURAL_PESSOA = TE.CO_CHV_NATURAL_PESSOA LEFT JOIN TP.CO_FAMILIAR_FAM ON TE.CO_FAMILIAR_FAM
WHERE TP.CO_CHV_NATURAL_PESSOA, TP.CO_FAMILIAR_FAM 
FROM P_CADASTRO_ODS_202206.TB_PESSOA_04 tp  
WHERE CO_PARENTESCO_RF_PESSOA IN ('1') AND CO_EST_CADASTRAL_MEMB IN (3,5,6);
